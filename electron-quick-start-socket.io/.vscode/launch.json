    "compounds": [
        {
            "name": "multi",
            "configurations": ["Debug Main Process","Debug Main Process1"]
        }
    ],
    "configurations": [
        {
            "name": "Debug Main Process",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
            },
           // "program": "${workspaceFolder}/main.js",
            "protocol": "legacy",
            "args": [
                "."
            ]
        },

        {
            "name": "Debug Main Process1",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
            },
           // "program": "${workspaceFolder}/main.js",
            "protocol": "legacy",
            "args": [
                "."
            ]
        }
    ]
